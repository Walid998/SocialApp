{
  "name": "inova-test",
  "description": "inova-test for social media application",
  "version": "0.0.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Walid Salah"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "test": "npm run jest",
    "serve": "ts-node-dev --no-notify src/",
    "start": "node dist/",
    "jest": "jest --coverage --coverageReporters=\"text-summary\" --forceExit",
    "build": "tsc",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "version": "git push && git push --tags"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "dist/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/transport-commons": "^4.5.18",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "feathers-hooks-common": "^7.0.3",
    "feathers-seeder": "^2.0.0",
    "feathers-sequelize": "^6.4.0",
    "feathers-swagger": "^2.0.0",
    "helmet": "^5.1.1",
    "pg": "^8.9.0",
    "sequelize": "^6.29.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@feathersjs/cli": "^4.8.0",
    "@types/bluebird": "^3.5.38",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/serve-favicon": "^2.5.3",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "axios": "^1.3.2",
    "babel-jest": "^29.4.2",
    "eslint": "^8.33.0",
    "jest": "^29.4.2",
    "sequelize-cli": "^6.6.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
